{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "headline": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "headline"
    },
    "resumen": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "figcaption": {
      "type": "string"
    }
  }
}
